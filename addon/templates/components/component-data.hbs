<br />
<table>
  <caption>
    <h3>
      {{@model.name}}
    </h3>
  </caption>
  <tbody>
    <tr>
      <td>
        <table>
          <thead>
            <tr>
              <th>
                Imports
              </th>
            </tr>
          </thead>
          <tbody>
            {{#each @model.jsImports as |prop|}}
              <tr>
                <td>
                  {{prop}}
                </td>
              </tr>
            {{/each}}
          </tbody>
        </table>
        <table>
          <thead>
            <tr>
              <th>
                Computeds
              </th>
            </tr>
          </thead>
          <tbody>
            {{#each @model.jsComputeds as |prop|}}
              <tr>
                <td>
                  {{prop}}
                </td>
              </tr>
            {{/each}}
          </tbody>
        </table>
        <table>
          <thead>
            <tr>
              <th>
                Props
              </th>
            </tr>
          </thead>
          <tbody>
            {{#each @model.jsProps as |prop|}}
              <tr>
                <td>
                  {{prop}}
                </td>
              </tr>
            {{/each}}
          </tbody>
        </table>
        <table>
          <thead>
            <tr>
              <th>
                Functions
              </th>
            </tr>
          </thead>
          <tbody>
            {{#each @model.jsFunc as |prop|}}
              <tr>
                <td>
                  {{prop}}
                </td>
              </tr>
            {{/each}}
          </tbody>
        </table>
        <table>
          <thead>
            <tr>
              <th>
                API
              </th>
              <th>
                Value
              </th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>
                tagName
              </td>
              <td>
                {{@model.api.tagName}}
              </td>
            </tr>
            <tr>
              <td>
                attributeBindings
              </td>
              <td>
                {{@model.api.attributeBindings}}
              </td>
            </tr>
            <tr>
              <td>
                mergedProperties
              </td>
              <td>
                {{@model.api.mergedProperties}}
              </td>
            </tr>
            <tr>
              <td>
                classNameBindings
              </td>
              <td>
                {{@model.api.classNameBindings}}
              </td>
            </tr>
            <tr>
              <td>
                concatenatedProperties
              </td>
              <td>
                {{@model.api.concatenatedProperties}}
              </td>
            </tr>
            <tr>
              <td>
                positionalParams
              </td>
              <td>
                {{@model.api.positionalParams}}
              </td>
            </tr>
            <tr>
              <td>
                classNames
              </td>
              <td>
                {{@model.api.classNames}}
              </td>
            </tr>
            {{#each @model.api.actions as |prop|}}
              <tr>
                <td>
                  @action
                </td>
                <td>
                  {{prop}}
                </td>
              </tr>
            {{/each}}
          </tbody>
        </table>
        <table>
          <thead>
            <tr>
              <th>
                Components Used
              </th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>
                {{@model.hbsComponents}}
              </td>
            </tr>
          </tbody>
        </table>
        <table>
          <thead>
            <tr>
              <th>
                Template props
              </th>
            </tr>
          </thead>
          <tbody>
            {{#each @model.hbsProps as |prop|}}
              <tr>
                <td>
                  {{prop}}
                </td>
              </tr>
            {{/each}}
          </tbody>
        </table>
      </td>
      <td></td>
    </tr>
  </tbody>
</table>